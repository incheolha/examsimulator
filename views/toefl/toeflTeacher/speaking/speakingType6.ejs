-->

<!--
      <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
      <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>>        
-->
<body onload=startTime()>
     <div class="ui blue borderless inverted menu" id="mainMenu">
          <div class="left menu">
            <a class="item">
              <h1>Toefl Simulator<br></h1> <br><br> <h4>for Teachers</h4>   
              </a>
            </div>      
        <div class="centered menu">  
            
          <a class="item">
           
            <i class="huge github icon"></i>
            </a>
  
        </div>     
        <div class="right menu">
        <a class="item" id="clock"> </a>    
       
    <div class="ui dropdown item">
       <i class="user icon"></i>
       <% if (user.permissionTag == 'toeflTeacher') { %>
         
          <%= user.name %> 선생님, 환영합니다.
		      <!-- <%= user.email %> -->
        <% } %>

      <i class="dropdown icon"></i>
      <div class="menu">
        <a class="item">profile</a>
         <a class="item" href="/logout">Logout</a>
            </div>
        </div>    
     </div>   
    </div>

          <div>
          <div class="ui huge breadcrumb">
          <a class="section" id="breadcrumbMain">문제출제하기</a>
          <i class="right chevron icon divider"></i>
          <a class="section"> <%= ExamNO %> 회차</a>
          <i class="right chevron icon divider"></i>  
          <a class="section">Speaking</a>
          <i class="right chevron icon divider"></i>  
          <div class="active section">Chapter <%=speakingProblemType %></div>  
         
         
    
        </div>
              <button class="teal ui right floated button" id="sp_chapter_complete_btn">챕터 완료</button>
         </div>
             
             
        <br>
             <br>
    
  <div class="ui inverted blue six item menu">
  <div class="item">
         <form class="ui form" action="/toefl/toeflTeacher/speaking" method="post" >
          <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
          <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>> 
          <input type="hidden" name="speakingProblemType" value=1>
       <button type="submit" class="ui primary button">Chapter1</button> 
           </form> 
  </div>
   <div class="item">
        <form class="ui form" action="/toefl/toeflTeacher/speaking" method="post" >
          <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
          <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>> 
          <input type="hidden" name="speakingProblemType" value=2>
       <button type="submit" class="ui primary button">Chapter2</button> 
           </form> 
  </div>  <div class="item">
         <form class="ui form" action="/toefl/toeflTeacher/speaking" method="post" >
          <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
          <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>> 
          <input type="hidden" name="speakingProblemType" value=3>
       <button type="submit" class="ui primary button">Chapter3</button> 
           </form> 
  </div>  <div class="item">
         <form class="ui form" action="/toefl/toeflTeacher/speaking" method="post" >
          <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
          <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>> 
          <input type="hidden" name="speakingProblemType" value=4>
       <button type="submit" class="ui primary button">Chapter4</button> 
           </form> 
  </div>  <div class="item">
         <form class="ui form" action="/toefl/toeflTeacher/speaking" method="post" >
          <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
          <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>> 
          <input type="hidden" name="speakingProblemType" value=5>
       <button type="submit" class="ui primary button">Chapter5</button> 
           </form> 
  </div>  <div class="item">
         <form class="ui form" action="/toefl/toeflTeacher/speaking" method="post" >
          <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
          <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>> 
          <input type="hidden" name="speakingProblemType" value=6>
       <button type="submit" class="ui primary button">Chapter6</button> 
           </form> 
  </div>
</div>



<% if (resultModifyTag == true) { %> 
  <% var a = speakingAnnouncementAudio; %>
  <% var b = speakingProblemReading; %>
  <% var c = speakingProblemListeningAudio; %>
  <% var d = speakingProblemListeningImage; %>
  <% var e = speakingProblem %>
  <% var f = speakingProblemAnswer %>

  <button  id="btn1"><%= a %></button>
  <button  id="btn2"><%= b %></button>
  <button  id="btn3"><%= c %></button>
  <button  id="btn4"><%= d %></button>
  <button  id="btn5"><%= e %></button>
  <button  id="btn6"><%= f %></button>
<% } %>

<div class="ui fluid container">
  


  <div class="ui two column divided grid">
  <div class="row">
    <div class="column">



        <div class="ui segment"  id="chapter2" >
       

     

            <form class="ui form" action="/process/toefl/speaking/multerAddSpeaking" id="speakingForm1" method="post" enctype="multipart/form-data">
              <div class="field">
            <input type="hidden" name="ExamNO" value=<%= ExamNO %>>
            <input type="hidden" name="ExamDesc" value=<%= ExamDesc %>> 
            <input type="hidden" name="speakingProblemType" value=<%= speakingProblemType %>>              
  
<!--
                <div class="ui tag labels">
                  <a class="ui top big blue label">
                    Academic Summary
                  </a>  
                </div>
-->
                  
            <div class="ui blue ribbon big label">
                <i class="spoon icon"></i> Academic Summary
            </div>                      
                  
        <h3 class="ui header">문제를 소개하는 Narrator mp3 파일을 올려주세요  </h3>       
        <label for="sp_ch_6_mp3_ipt" class="ui icon button">
            <i class="file icon"></i>
            Open mp3 File</label>
            <input type="file" id="sp_ch_6_mp3_ipt" name="speakingAnnouncementAudio" style="display:none" accept="audio/*" multiple>
            <div id="sp_ch_6_mp3_pre">  
              </div>             
                  
     

              </div>
                
                
              <div class="field">
    
        <h3 class="ui header">Paired Choice 에 관련된 문제를 입력해주세요  </h3> 
        <textarea  id="editor1" name="spekaingProblemReading"></textarea>
                  
       
          
                
              </div>
              
             <div class="field">
        
        <h3 class="ui header"> Lecture mp3/img 파일 업로드하기   </h3>      
            <label for="sp_ch_6_img_ipt" class="ui icon button">
                 <i class="file icon"></i>
                Open img File</label>
            <input type="file" id="sp_ch_6_img_ipt" id="speakingProblemListeningImage" style="display:none" accept="image/*" multiple>
            <div id="sp_ch_6_img_pre">
            </div> 
            <br>
            <br>  
              <label for="sp_ch_6_1_mp3_ipt" class="ui icon button">
                 <i class="file icon"></i>
                Open mp3 File</label>
            <input type="file" id="sp_ch_6_1_mp3_ipt" name="speakingProblemListeningAudio" style="display:none" accept="audio/*" multiple>
            <div id="sp_ch_6_1 _mp3_pre">  
              </div>   
                
                
                
                </div>                                
             
            <div class='field'>
                <h3 class="ui header">  문제 입력하기   </h3>   
                <input type="text" class="ui input" name="speakingProblem" placeholder="문제를 입력해 주세요 ">
                
                 <h3 class="ui header"> 문제 해설작성하기  </h3>   
                <input type="text" class="ui input" name="speakingProblemAnswer" placeholder="해설을 입력해 주세요 ">
                
                
                </div>    
                
                
                                
                                
              <button class="ui blue button" type="submit">Submit</button>
            </form>  
        </div>
       </div>
     <% if (resultModifyTag == true) { %> 

    <div class="eight wide column">
      <div class="ui raised segment">
           <div class="ui blue ribbon big label">
                <i class="spoon icon"></i> 저장된 문제
            </div>  
           <br>
            
         <h3 class="ui header"></h3> 
        <table class="ui fluid attached blue large  table">
        <thead>
          <tr><th>Subject Name</th>
          <th>Saved Value</th>
         
        </tr></thead><tbody>
         
          <tr>
            <td>Announcement Audio</td>
          
            <td><%= a %></td>
        
          </tr>
          <tr>
            <td>Problem Audio</td>
       
            <td><%= c %></td>
        
          </tr>
           <tr>
            <td>Problem Image</td>
       
            <td><%= d %></td>
        
          </tr>
           <tr>
            <td>Problem</td>
          
            <td> <%= e %></td>
        
          </tr>
          <tr>
       
            <td>Problem Desc.</td>
            <td> <%= f %></td>
       
          </tr>
        </tbody>
        
      </table>
      <br>  

    <div class="ui bottom info message">
        
        <div class="header">
          Was this what you wanted?
        </div>
        <ul class="list">
          <li>It's good to see you again.</li>
          <li>Did you know it's been a while?</li>
        </ul>
      </div>



    </div>
  </div>
 
  </div>
</div>
     <% } else { %>
   <div class="eigth wide column">
      
  <div class="ui bottom info message">
        
      <div class="header">
        아직 저장된 내용이 없네요!
      </div>
      <ul class="list">
        <li>알고 계셨나요??</li>
        <li>문제를 만들고 저장하시면 이부분에서 문제가 나타납니다</li>
          
      </ul>
    </div>
  </div>
 <% } %>    


		<div class="ui modal" id="go_back_main_modal">
          <i class="close icon"></i>
          <div class="header">
			
  
			  
           메인 화면으로 이동하기
          </div>
          <div class="content">
         
            <div class="description">
              <div class="ui header">
				 
				  문제 출제하기 메인 화면으로 이동합니다.</div>
           
            </div>
          </div>
          <div class="actions">
         
              <button class="ui grey deny button">이전으로</button>
              
              <button class="ui blue button">이동하기</button>
            
          </div>
        </div> 		 		 
			 
			 

</body>
    
    
</html>    

            


   